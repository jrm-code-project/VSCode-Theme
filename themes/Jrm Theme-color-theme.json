    "tokenColors": [
		{
			"name": "Angle brackets",
			"scope": [
				"punctuation.bracket.angle"
			],
			"settings": {
				"foreground": "#00CF00"
			}
		},
		{
			"name": "Arithmetic",
			"scope": [
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#AFAFCF"
			}
		},
		{
			"name": "Arrow",
			"scope": [
				"storage.type.function.arrow"
			],
			"settings": {
				"foreground": "#0000CF"
			}
		},
		{
			"name": "Block braces",
			"scope": [
				"punctuation.section.block.begin.bracket.curly",
				"punctuation.section.block.end.bracket.curly"
			],
			"settings": {
				"foreground": "#3Faf00"
			}
		},
		{
			"name": "Class braces",
			"scope": [
				"punctuation.section.class.begin.bracket.curly",
				"punctuation.section.class.end.bracket.curly",
				"punctuation.section.enum.begin.bracket.curly",
				"punctuation.section.enum.end.bracket.curly",
				"punctuation.section.inner-class.begin.bracket.curly",
				"punctuation.section.inner-class.end.bracket.curly"
			],
			"settings": {
				"foreground": "#5FAF00"
			}
		},
		{
			"name": "Class, enum and method definition",
			"scope": [
				"meta.enum entity.name.type.enum",
				"meta.class.body meta.method meta.method.identifier entity.name.function",
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#CF00CF"
			}
		},
		// {
		// 	"name": "Class variable definition",
		// 	"scope": [
		// 		"meta.class.body keyword.operator.assignment"
		// 	],
		// 	"settings": {
		// 		"foreground": "#00AF00"
		// 	}
		// },
		// {
		// 	"name": "Class variables",
		// 	"scope": [
		// 		"meta.class.body meta.definition.variable"
		// 	],
		// 	"settings": {
		// 		"fontStyle": "bold",
		// 		"foreground": "#00CF00"
		// 	}
		// },
		{
			"name": "Code",
			"scope": [
				"meta.class.body",
                // "meta.method.body",
                "meta.function-call entity.name.function",
                "meta.method-call entity.name.function",
                "source.python",
				"variable.other.object",
				//"variable.parameter"
			],
			"settings": {
				"foreground": "#00CF00"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment.block",
                "comment.line",
                "string.quoted.docstring.multi.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#03CFCF"
			}
		},
		{
			"name": "Comment javadoc keywords",
			"scope": [
				"comment.block keyword.other.documentation"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#cf5F00"
			}
		},
		{
			"name": "Comment javadoc parameters",
			"scope": [
				"comment.block variable.parameter"
			],
			"settings": {
				"fontStyle":"",
				"foreground": "#00CF00"
			}
		},
		{
			"name": "Constants",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#CfAFAF"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
                "keyword.control.ternary",
                "keyword.operator.assignment",
                "punctuation.bracket.square",
				"punctuation.separator.delimiter",
				"punctuation.separator.period",
				"punctuation.terminator"
			],
			"settings" : {
				"foreground": "#00AF00"
			}
        },
        {
            "name": "Invalid",
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#FF0000"
            }
        },
		{
			"name": "Keywords",
			"scope": [
				"keyword.control",
				"keyword.other.import",
				"keyword.other.package",
				"variable.language"
			],
			"settings": {
				"foreground": "#CF5F00"
			}
		},
		{
			"name": "Logical and relational operators",
			"scope": [
				"keyword.operator.comparison",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#7F7FCF"
			}
		},
		{
			"name": "Method Braces",
			"scope": [
				"punctuation.section.method.begin.bracket.curly",
				"punctuation.section.method.end.bracket.curly"
			],
			"settings": {
				"foreground":"#00AF00"
			}
		},
		{
			"name": "Numbers and Enums",
			"scope": [
				"constant.other.enum",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#CFCFAF"
			}
		},
		{
			"name": "Package and Imports",
			"scope": [
				"storage.modifier.import",
				"storage.modifier.package"
			],
			"settings": {
				"foreground": "#CFCFCF"
			}
		},
		{
			"name": "Parenthesis",
			"scope": [
				"punctuation.bracket.round",
				"punctuation.definition.parameters.begin.bracket.round",
				"punctuation.definition.parameters.end.bracket.round"
			],
			"settings": {
				"foreground": "#00AF00"
			}
		},
		// {
		// 	"name": "Property reference",
		// 	"scope": [
		// 		"variable.other.object.property"
		// 	],
		// 	"settings": {
		// 		"foreground": "#5f5fAF"
		// 	}
		// },
		{
			"name": "Side effects",
			"scope": [
				"keyword.operator.increment-decrement"
			],
			"settings": {
				"foreground":"#cf1f1f"
			}
		},
		{
			"name": "Storage modifiers",
			"scope": [
				"storage.modifier"
			],
			"settings": {
				"foreground": "#cf8F00"
			}
		},
		{
			"name": "Strings",
			"scope": [
                "string.quoted.double",
                "string.quoted.single"
			],
			"settings": {
				"foreground": "#cfcfaf"
			}
		},
		{
			"name": "Try, catch, finally",
			"scope": [
				"punctuation.section.catch.begin.bracket.curly",
				"punctuation.section.catch.end.bracket.curly",
				"punctuation.section.finally.begin.bracket.curly",
				"punctuation.section.finally.end.bracket.curly",
				"punctuation.section.try.begin.bracket.curly",
				"punctuation.section.try.end.bracket.curly"
			],
			"settings": {
				"foreground": "#00af57"
			}
        },
        {
            "name": "Tag",
            "scope": [
                "entity.name.tag",
                "meta.tag.sgml",
                "markup.deleted.git_gutter"
            ],
            "settings": {
                "foreground": "#00AC00"
            }
        },
		{
			"name": "Types",
			"scope": [
                "entity.other.inherited-class",
				"punctuation.definition.annotation",
				"storage.type"
			],
			"settings": {
				"foreground": "#CFCF00"
			}
        },
        {
            "name": "XML",
            "scope": [
                "text.xml"
            ],
            "settings": {
                "foreground": "#CFCFCF"
            }
        },
        {
            "name": "XML attributes",
            "scope": [
                "entity.other.attribute-name.xml"
            ],
            "settings": {
                "foreground": "#CFCF00"
            }
        },
        {
            "name": "XML localnames",
            "scope": [
                "entity.other.attribute-name.localname.xml"
            ],
            "settings": {
                "foreground": "#00CFCF"
            }
        },
        {
            "name": "XML namespace",
            "scope": [
                "entity.other.attribute-name.namespace.xml"
            ],
            "settings": {
                "foreground": "#FC7f7f"
            }
        },
        {
            "name": "XML punctuation",
            "scope": [
                "punctuation.definition.tag.xml"
            ],
            "settings": {
                "foreground": "#007F00"
            }
        },

